<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google ncr</os:ShortName>
<os:Description>Google ncr</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<os:Url type="text/html" method="GET" template="https://www.google.com/search" resultDomain="google.com">
  <os:Param name="num" value="20"/>
  <os:Param name="newwindow" value="1"/>
  <os:Param name="safe" value="off"/>
  <os:Param name="site" value=""/>
  <os:Param name="source" value="hp"/>
  <os:Param name="btnG" value="Search"/>
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="oq" value=""/>
  <os:Param name="gs_l" value=""/>
</os:Url>
</SearchPlugin>